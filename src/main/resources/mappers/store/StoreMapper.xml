<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.plateer.fittingroombo.store.mapper.StoreMapper">

    <select id="getStoreStatus" resultType="SellerDTO">
        select se_status
        from tbl_seller
        where se_no = #{seNo}
    </select>

</mapper>